<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="QQStepView">
        <!--        外圆颜色-->
        <attr name="outerCircleColor" format="color" />
        <!--        内圆颜色-->
        <attr name="innerCircleColor" format="color" />
        <!--        步数文字颜色-->
        <attr name="stepTextColor" format="color" />
